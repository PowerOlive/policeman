    "gulp-jison": "^1.2.0",
    "gulp-raster": "^0.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.7",
    "jpm": "^1.0.7",
    "lodash": "^4.12.0",
    "minimist": "^1.2.0",
    "process-promises": "^1.0.0",
    "q": "^1.4.1",
    "split": "^1.0.0",
    "which-promise": "^1.0.0"
  },
  "dependencies": {}
